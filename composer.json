{
  "name": "tourze/json-rpc-encrypt-bundle",
  "description": "JsonRPC加解密实现",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-hash": "*",
    "ext-openssl": "*",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/polyfill-php83": "^1.31",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/json-rpc-caller-bundle": "0.1.*",
    "tourze/json-rpc-core": "^0.0.6"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCEncryptBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCEncryptBundle\\Tests\\": "tests"
    }
  }
}