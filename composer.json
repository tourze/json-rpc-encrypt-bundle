{
  "name": "tourze/json-rpc-encrypt-bundle",
  "description": "JsonRPC加解密实现",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "ext-openssl": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
